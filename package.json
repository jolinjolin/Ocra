    "got": "^12.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18"
  }
